import 'package:flutter/material.dart';

class MyTeamDetailsResponse extends APIResponse {
  MyDetails myTeamDetails;

  MenuItem({this.myTeamDetails});
}




class MyDetails {
 List<MyTeamDetails> teamDetails;

  MenuItem({this.teamDetails});
}



class MyTeamDetails{
	private String personNumber;
	private String legacyEmployeeNumber;
	private String userName;
	private String fullName;
	private String assignmentStatus;
	private String positionName;
	private String assignmentOrganization;
	private String location;
	private String personType;
	private String contractType;
	private String dateOfBirth;
	private String originalHireDate;
	private String lengthOfService;
	private String grade;
	private String gender;
	private String nationality;
	private String courseCount;
	private String teamSize;
	private List<PerformanceHistory> performanceHistory;
	private String managerName;

	 MyTeamDetails({this.personNumber,this.legacyEmployeeNumber,this.userName,this.fullName,this.assignmentStatus,this.positionName,this.assignmentOrganization,this.location,this.personType,this.contractType,this.dateOfBirth,this.originalHireDate,this.lengthOfService,this.grade,this.gender,this.nationality,this.courseCount,this.teamSize,this.performanceHistory,this.managerName});
}
